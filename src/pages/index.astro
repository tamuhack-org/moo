        <!-- horizontal-timeline scripts -->
        <script is:inline>
            if (typeof swipeContentTimeline === 'function')
                swipeContentTimeline();
            if (typeof initalizeHoriTimeline === 'function')
                initalizeHoriTimeline();
            // Party Mode Logic
            const partyBtns = document.querySelectorAll('.party-trigger-btn');
            let isParty = false;

            partyBtns.forEach((btn) => {
                btn.addEventListener('click', (e) => {
                    e.preventDefault();

                    isParty = !isParty;

                    const normalImages = document.querySelectorAll(
                        '.director-media.normal-state.has-party-alt',
                    );
                    const partyImages = document.querySelectorAll(
                        '.director-media.party-state',
                    );

                    if (isParty) {
                        normalImages.forEach(
                            (el) => (el.style.display = 'none'),
                        );
                        partyImages.forEach(
                            (el) => (el.style.display = 'block'),
                        );
                    } else {
                        normalImages.forEach(
                            (el) => (el.style.display = 'block'),
                        );
                        partyImages.forEach(
                            (el) => (el.style.display = 'none'),
                        );
                    }
                });
            });
        </script>

